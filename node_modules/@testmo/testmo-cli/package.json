{
  "name": "@testmo/testmo-cli",
  "version": "1.0.13",
  "description": "Testmo command line interface",
  "author": "Testmo GmbH",
  "homepage": "https://www.testmo.com/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "testmo": "dist/index.js"
  },
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
    "lint": "npm run compile && npx eslint \"src/**/*.ts\"",
    "lint-fix": "npm run compile && npx eslint --fix \"src/**/*.ts\"",
    "compile": "tsc -p tsconfig.json",
    "test": "node_modules/mocha/bin/mocha",
    "test-ci": "node_modules/mocha/bin/mocha --reporter node_modules/mocha-junit-reporter --reporter-options jenkinsMode=1,mochaFile=results/results.xml",
    "clean": "rm -rf dist/ && rm -f testmo*.tgz",
    "prepack": "npm run clean && npm run compile"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cli-progress": "^3.9.1",
    "@types/glob": "^7.1.3",
    "@types/he": "^1.1.2",
    "@types/lodash": "^4.14.168",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.13",
    "@types/sinon": "^10.0.6",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.0",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@testmo/testmo-api": "^1.0.0",
    "chalk": "^4.1.0",
    "cli-progress": "^3.9.0",
    "commander": "^8.3.0",
    "fast-xml-parser": "^3.19.0",
    "glob": "^7.1.6",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.29",
    "validator": "^13.5.2"
  }
}
